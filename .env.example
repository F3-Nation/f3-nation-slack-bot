# All variables are plain KEY=VALUE (no "export"). Docker Compose and tools that read .env expect this format.

# Slack
SLACK_SIGNING_SECRET=your-signing-secret
SLACK_BOT_TOKEN=your-bot-token

# Database (local dev defaults)
DATABASE_HOST=localhost
DATABASE_USER=postgres
DATABASE_PASSWORD=postgres
DATABASE_SCHEMA=f3_schema
POSTGRES_PASSWORD=${DATABASE_PASSWORD}
POSTGRES_DB=${DATABASE_SCHEMA}
SQL_ECHO=False

# App behavior
LOCAL_DEVELOPMENT=True
LOCAL_HOST_DOMAIN=your-app.local

# Optional/legacy admin DB vars (keep if referenced by scripts or compose)
ADMIN_DATABASE_USER=root
ADMIN_DATABASE_PASSWORD=your-db-password
ADMIN_DATABASE_SCHEMA=f3
DB_ADMIN_PASSWORD=set-yourself

